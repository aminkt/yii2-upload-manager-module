{
	"name": "aminkt/yii2-upload-manager",
	"description": "Upload manager system based and inspired from Wordpress upload manager.",
  	"keywords": ["yii", "upload manager", "upload", "module", "manager"],
	"type": "yii2-extension",
	"support": {
		"source": "https://gitlab.com/aminkt/yii2-uploadManager-module"
	},
	"authors": [
		{
			"name": "Amin Keshavarz",
			"email": "ak_1596@yahoo.com"
		}
	],
	"require": {
		"yiisoft/yii2": "*",
        "perminder-klair/yii2-dropzone": "*",
        "yiisoft/yii2-imagine": "~2.0.0",
		"faravaghi/yii2-jalali-datepicker": "*"
	},
	"autoload": {
		"psr-4": {"aminkt\\uploadManager\\": "src"}
	},
	"autoload-dev": {
		"psr-4": {"aminkt\\uploadManager\\test": "test"}
	}
}
